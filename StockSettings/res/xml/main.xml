<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="@string/category_s2w_title">
        <CheckBoxPreference android:title="@string/s2w_switch_title_head" android:key="s2w_switch" android:summary="@string/s2w_switch_summary_head" android:defaultValue="1" />
        <ListPreference android:entries="@array/s2w_stroke_entries" android:title="@string/s2w_stroke_title_head" android:key="s2w_stroke" android:summary="@string/s2w_stroke_summary_head" android:dependency="s2w_switch" android:defaultValue="1" android:entryValues="@array/s2w_stroke_entries_values" />
`	<ListPreference android:entries="@array/s2w_length_entries" android:title="@string/s2w_length_title_head" android:key="s2w_length" android:summary="@string/s2w_length_summary_head" android:dependency="s2w_switch" android:defaultValue="325" android:entryValues="@array/s2w_length_entries_values" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/category_usb_title">
        <CheckBoxPreference android:title="@string/fastcharge_title_head" android:key="fastcharge" android:summary="@string/fastcharge_summary_head" android:defaultValue="0" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/category_backlightdisable_title">
        <CheckBoxPreference android:title="@string/backlightdisable_title_head" android:key="backlight_disable" android:summary="@string/backlightdisable_summary_head" android:defaultValue="0" />
        <CheckBoxPreference android:title="@string/backlightnotification_title_head" android:key="backlight_notification" android:summary="@string/backlightnotification_summary_head" android:defaultValue="0" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/category_smartdimmer_title">
        <CheckBoxPreference android:title="@string/smartdimmer_switch_title_head" android:key="smartdimmer_switch" android:summary="@string/smartdimmer_switch_summary_head" android:defaultValue="0" />
    </PreferenceCategory>
</PreferenceScreen>
