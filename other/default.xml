  <BootConfiguration>
    <BootAnimation animation="/system/media/bootanimation.zip" audio="/system/media/bootaudio.mp3" />
  </BootConfiguration>
  <category name="SettingsProvider">
    <module name="res">
      <function name="values">
        <set name="single">
          <item name="def_screen_off_timeout">60000</item>
          <item name="def_2g_on">3</item>
        </set>
      </function>
    </module>
  </category>
  <category name="AutomotiveHome">
    <module name="setting">
      <function>
        <set name="single">
          <item name="default_Location">HTC Locations</item>
        </set>
      </function>
    </module>
  </category>
  <category name="Settings">
    <module name="properties">
      <function>
        <set name="single">
          <!--regulation.png-->
          <item name="support_regulatory">true</item>
        </set>
      </function>
    </module>
  </category>
  <category name="framework">
    <module name="ShareVia">
